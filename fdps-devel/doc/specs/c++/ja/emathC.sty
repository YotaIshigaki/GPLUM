  \else
    \Sub{1}\ik@sum@\ik@ast
    \Strsep{#2}{*}\ik@a\ik@b
    \EMedef\ik@tmp{\ik@a\ik@ast\ik@b}%
    \itiziketugou<#1>\ik@tmp#3\relax
  \fi
}%
