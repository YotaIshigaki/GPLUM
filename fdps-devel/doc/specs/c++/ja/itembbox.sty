\def\removept#1#2{%
    \edef#2{\expandafter\@removePT\the#1}}%
{\catcode`p=12 \catcode`t=12 \gdef\@removePT#1pt{#1}}%
\def\keytop#1{{\leavevmode\kern1pt
  \setbox1=\hbox{\normalfont\sffamily [}%
  \dimen1=\ht1
  \removept{\dimen1}{\tempC}%
  \setbox0=\hbox{\normalfont\sffamily #1}%
