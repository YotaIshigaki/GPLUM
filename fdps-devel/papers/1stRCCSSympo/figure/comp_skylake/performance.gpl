set term post enh color eps 24

set style fill solid border lc rgb "black"
set boxwidth 0.5 relative

set yrange [0:]

fact = 1.8/2.3

set ylabel "Performance / GFlops"
set output "../comp_skylake_performance.eps"
p "performance.dat" u 0:3:xtic(1) w boxes lw 2 not

set key left
set ylabel "Elapsed time per interaction / ns"
set output "../comp_skylake_elapsed_time.eps"
p "performance.dat" u 0:($2/$4*10**9):xtic(1) w boxes lw 2 not


