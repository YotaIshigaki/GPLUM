set term post enh color eps 36
set border 15 lw 4
set grid x lw 4

set size 2,0.5
set logscale x 2
set xrange [2**(-7):2**(9)]
set format x "2^{%L}"
set format y ""
set xtics (2**(-7),2**(-6),2**(-5),2**(-4),2**(-3),2**(-2),2**(-1),1,2,4,8,16,32,64,128,256,512)

f(x) =  sqrt(erfc(sqrt(x))/x + 2*exp(-x)/sqrt(x)/3.141592)
set output "interval.eps"
p f(x) not lw 8,"interval.dat" not pt 7 lc 1 ps 2 

